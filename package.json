{
  "name": "dnb-hugo-head",
  "version": "0.0.9",
  "license": "MIT",
  "description": "DNB's answer to the question 'What tags belong into the <head> tag of my website?'",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com> (https://davids-neighbour.com)",
  "homepage": "https://github.com/dnb-org/dnb-hugo-head#readme",
  "repository": "dnb-org/dnb-hugo-head",
  "bugs": "https://github.com/dnb-org/dnb-hugo-head/issues",
  "dependencies": {
    "mem": "^9.0.1",
    "trim": "^1.0.1"
  },
  "devDependencies": {
    "@dnb-org/standard-version-config": "^3.4.7",
    "@dnb-org/tools": "^3.4.9"
  },
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release-hook-postrelease",
    "release:major": "standard-version --release-as major -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release-hook-postrelease",
    "release:minor": "standard-version --release-as minor -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release-hook-postrelease"
  },
  "private": true,
  "slug": "dnb-hugo-head"
}
